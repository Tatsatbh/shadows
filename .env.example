# OpenAI API Key for realtime voice features
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
# Get these from your Supabase project settings: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Supabase Service Role Key (for admin operations only - keep secure!)
# Only needed for server-side admin operations. Do NOT expose to client.
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Judge0 API Key for code execution
JUDGE0_API_KEY=your_judge0_api_key_here

# Note: NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY appears to be a duplicate/legacy key
# The standard Supabase setup uses NEXT_PUBLIC_SUPABASE_ANON_KEY
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
